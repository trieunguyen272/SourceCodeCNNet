@model List<Models.Framework.KHUNHA>
@{
    ViewBag.Title = "SearchByName";
    ViewBag.Heading = "DANG SÁCH KHU NHÀ TÌM ĐƯỢC";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<table class="table" style="margin-top: 20px">
    <tr>
        <th>
            Mã khu
        </th>
        <th>
            Tên khu
        </th>
        <th>
            Mô tả khác
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MAKHU)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TENKHU)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MOTAKHAC)
            </td>
            <td>
                @Html.ActionLink("Sửa", "Edit", new { id = item.MAKHU }) |
                @Html.ActionLink("Chi tiết", "Details", new { id = item.MAKHU })

            </td>
        </tr>
    }

</table>

