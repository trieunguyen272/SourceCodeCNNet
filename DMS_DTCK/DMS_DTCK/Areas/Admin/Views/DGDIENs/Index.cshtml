@model IEnumerable<Models.Framework.DGDIEN>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h4 style="text-align: center">DANH SÁCH ĐƠN GIÁ ĐIỆN</h4>

<p style="text-align: center">
    @Html.ActionLink("Thêm mới đơn giá điện", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MADGDIEN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DONGIADIEN)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MADGDIEN)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DONGIADIEN)
        </td>
        <td>
            @Html.ActionLink("Sửa", "Edit", new { id=item.MADGDIEN }) |
            @Html.ActionLink("Chi tiết", "Details", new { id=item.MADGDIEN }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.MADGDIEN })
        </td>
    </tr>
}

</table>
