@model List<Models.Framework.HDDIENNUOC>
@{
    ViewBag.Title = "SearchByName";
    ViewBag.Heading = "DANG SÁCH HÓA ĐƠN ĐIỆN NƯỚC TÌM ĐƯỢC";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<table class="table" style="margin-top: 20px">
    <tr>
        <th>
            Mã phòng
        </th>
        <th>
            Tháng
        </th>
        <th>
            Năm
        </th>
        <th>
            Chỉ số điện cũ
        </th>
        <th>
            Chỉ số điện mới
        </th>
        <th>
            Đơn giá điện
        </th>
        <th>
            Tiền điện
        </th>
        <th>
            Chỉ số nước cũ
        </th>
        <th>
           Chỉ số nước mới
        </th>
        <th>
            Đơn giá nước
        </th>
        <th>
            Tiền nước
        </th>
        <th>
            Tổng tiền
        </th>
        <th>
            Trạng thái
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PHONG.MAPHONG)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.THANG)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NAM)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CHISODIENCU)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CHISODIENMOI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DGDIEN.DONGIADIEN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TIENDIEN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CHISONUOCCU)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CHISONUOCMOI)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DGNUOC.DONGIANUOC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TIENNUOC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TONGTIEN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TRANGTHAITHANHTOAN)
            </td>
            <td>
                @Html.ActionLink("Sửa", "Edit", new { id = item.MAHOADON }) |
                @Html.ActionLink("Chi tiết", "Details", new { id = item.MAHOADON }) |
                @Html.ActionLink("Xóa", "Delete", new { id = item.MAHOADON })
            </td>
        </tr>
    }

</table>

